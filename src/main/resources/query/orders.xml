<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dotori.client.orders.dao.OrdersDao">
<insert id="ordersInsert" parameterType="orders">
	insert into spring_board(
		orders_num,member_id,project_num,orders_content,
		orders_price,orders_date,orders_guideAgree,
		orders_infoAgree,content_kind
	) 
	values(orders_seq.nextval,#{member_id},project_seq.nextval,#{orders_content},
		#{orders_price},#{orders_date},#{orders_guideAgree},
		#{orders_infoAgree},#{content_kind})
</insert>
</mapper>